                    <a href="" class="button">
                      <i class="fa fa-check"></i>
                      <span class="text">Yes</span>
                    </a>
                  </li>
                  <li class="">
                    <a href="" class="button">
                      <i class="fa fa-check"></i>
                      <span class="text">No</span>
                    </a>
                  </li>
                  <li class="">
                    <a href="" class="button">
                      <i class="fa fa-check"></i>
                      <span class="text">Maybe</span>
                    </a>
                  </li>
                </ul>
              </div>
            </div>
          </div>
